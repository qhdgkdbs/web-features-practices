{
  "compilerOptions": {
    "baseUrl": ".",
    "paths": {
      "@src/*": [
        "src/*"
      ],
      "@components/*": [
        "src/components/*"
      ],
      "@pages/*": [
        "src/pages/*"
      ],
      "@data-type/*": [
        "src/types/*"
      ],
      "@interfaces/*": [
        "src/interfaces/*"
      ],
      "@store/*": [
        "src/store/*"
      ],
      "@layout/*": [
        "src/layout/*"
      ],
      "@element/*": [
        "src/components/element/*"
      ],
      "@utils/*": [
        "src/utils/*"
      ]
    }
  }
}